{
  "name": "intuit-ui-craft-demonstration",
  "description": "Test Project for Intuit",
  "version": "0.0.0",
  "license": "MIT",
  "main": "lib/index.js",
  "author": "Rahat Ahmed",
  "repository": "rahatarmanahmed/intuit-ui-craft-demonstration",
  "scripts": {
    "lint": "eslint src/ test/",
    "test": "ava -r babel-register test/",
    "build:js": "babel -d lib/ src/",
    "build:html": "cpx src/index.js lib/",
    "build": "npm run build:js && npm run build:html",
    "pretest": "npm run lint",
    "watch": "onchange src/ -- npm run build && echo Done",
    "dev": "npm run watch",
    "prepublish": "npm run test && npm run build"
  },
  "devDependencies": {
    "ava": "^0.15.2",
    "babel-cli": "^6.10.1",
    "babel-eslint": "^6.1.2",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-stage-0": "^6.5.0",
    "babel-register": "^6.9.0",
    "cpx": "^1.3.1",
    "eslint": "^3.0.1",
    "onchange": "^2.5.0"
  },
  "dependencies": {
    "lodash": "^4.13.1",
    "react": "^15.2.1",
    "react-dom": "^15.2.1",
    "redux": "^3.5.2",
    "weather-icons": "^1.3.2"
  }
}
