{
    "parser": "babel-eslint",
    "extends": ["kellyirc", "plugin:react/recommended"],
    "plugins": ["react"],
    "env": {
        "es6": true,
        "browser": true,
        "commonjs": true
    },
    "parserOptions": {
        "ecmaFeatures": {
            "jsx": true
        }
    },
    "rules": {
        "react/prop-types": 0
    }
}
